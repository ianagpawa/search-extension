{
  "manifest_version": 2,

  "name": "Google Search Extension",
  "description": "This extension shows the titles of the results of a Google search.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "http://www.google.com/",
    "https://www.google.com/"
  ]
}
